{
  "name": "remote-audio-controller-server",
  "version": "0.3.0",
  "description": "A server that plays audio on the server",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx nodemon -e ts src/index.ts",
    "build": "npx tsc && rm -r ../ui/src/models/ && cp -r ./src/models/ ../ui/src/models/",
    "package": "tar -cvf remote-audio-controller-server package.json package-lock.json dist/* && gzip -S .tgz remote-audio-controller-server",
    "start": "node dist/index.js"
  },
  "author": "Matt Nowakowski",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "find-exec": "^1.0.3",
    "multer": "^2.0.2",
    "music-metadata": "^11.9.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/find-exec": "^1.0.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.0",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
